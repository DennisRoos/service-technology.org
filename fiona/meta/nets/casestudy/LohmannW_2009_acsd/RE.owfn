PLACE
INTERNAL a0, a1, a2, a3, a4, a5, a6, a7, a8;
INPUT x, y, z;
OUTPUT a, b, c;

INITIALMARKING a0: 1;
FINALCONDITION (a5=1 OR a7=1 OR a8=1) AND ALL_OTHER_PLACES_EMPTY;

TRANSITION t1
CONSUME a0, x;
PRODUCE a1;

TRANSITION t2
CONSUME a1;
PRODUCE a6;

TRANSITION t3
CONSUME a6;
PRODUCE a7, c;

TRANSITION t4
CONSUME a7, z;
PRODUCE a8;

TRANSITION t5
CONSUME a1;
PRODUCE a2;

TRANSITION t6
CONSUME a2;
PRODUCE a3, a;

TRANSITION t7
CONSUME a3;
PRODUCE a4, b;

TRANSITION t8
CONSUME a4, y;
PRODUCE a5;

