{ Annahmen: }
{- Flanken können sich nicht überholen}
{- zu Beginn alle Pegel auf 0, bis auf Pegel, denen Negator vorgeschaltet ist}
{- im clock generator Ausgang von NOR-Gatter auf 1}

{DONE:}
{  12->13:}
{     * Verzweigungstransitionen entfernt}
{     * Negationstransitionen entfernt}
{     * Platz p.DLE entfernt}

SORT Anzahl =  [ 0 , 2 ];

FUNCTION inc(x: Anzahl): Anzahl
BEGIN
	RETURN x + 1
END

FUNCTION reset(x: Anzahl): Anzahl
BEGIN
	RETURN 0
END

FUNCTION zero(): Anzahl
BEGIN
	RETURN 0
END


FUNCTION tt(): BOOLEAN
BEGIN
	RETURN TRUE
END


FUNCTION ff(): BOOLEAN
BEGIN
	RETURN FALSE
END


FUNCTION und(x : BOOLEAN; y : BOOLEAN): BOOLEAN
BEGIN
	RETURN x AND y
END


FUNCTION oder(x : BOOLEAN; y : BOOLEAN): BOOLEAN
BEGIN
	RETURN x OR y
END


FUNCTION neg(x : BOOLEAN): BOOLEAN
BEGIN
    RETURN NOT x
END



PLACE
	p.REQ_A : BOOLEAN,
	p.n_REQ_A,
	p.ACK_A : BOOLEAN,
	p.n_ACK_A,
	p.REQ_B : BOOLEAN,
	p.n_REQ_B,
	{p.ACK_B = op.m4.p.ack_b, op.m5.p.b}


	{Timeout-Generator (tog)}

	{tog.p.REQ_A = p.REQ_A}
	{tog.p.LCLK = neg(tog.m20.p.clk)}
	{tog.p.RST = tog.m21.p.clk, tog.m27.p.rst}
	{tog.p.REQ_A1 = ip.p.REQ_A1}
	{tog.p.ST = ip.p.ST, and.p.a}

	{m20: D-FF mit D=1 und RST}
	tog.m20.p.clk : BOOLEAN,   		{Input-Flanken}
	tog.m20.p.n_clk,
	tog.m20.p.rst : BOOLEAN,
	tog.m20.p.n_rst,
	tog.m20.p.q : BOOLEAN,     		{Output-Flanke}
	tog.m20.p.pegel_0_0,       		{Pegel: (rst, q)}
	tog.m20.p.pegel_0_1,
	tog.m20.p.pegel_1_0,
	tog.m20.p.n_q,             		{Komplementärplatz}

	{m21: D-FF mit D=1 und RST}
	tog.m21.p.clk : BOOLEAN,   		{Input-Flanke}
	tog.m21.p.rst : BOOLEAN,
	tog.m21.p.q: BOOLEAN,      		{Output-Flanke}
	tog.m21.p.pegel_0_0,       		{Pegel: (rst, q)}
	tog.m21.p.pegel_0_1,
	tog.m21.p.pegel_1_0,
	tog.m21.p.n_q,
	tog.m21.p.n_clk,
	tog.m21.p.n_rst,

	{m22: Mutex}
	{tog.m22.p.a_in = tog.m20.p.q}
	{tog.m22.p.b_in = tog.m21.p.q}
	{tog.m22.p.a_out = tog.m20.p.rst, tog.m23.p.clk}   {Output: Flanke}
	{tog.m22.p.b_out = tog.m21.p.rst, tog.m25.p.a, tog.m23.p.rst: b}
	tog.m22.p.pegel_0_0_0_0,                     {zu speichernde Pegel}
	tog.m22.p.pegel_0_1_0_1,
	tog.m22.p.pegel_1_0_1_0,
	tog.m22.p.pegel_1_1_0_1,
	tog.m22.p.pegel_1_1_1_0,

	{m23: Counter}
	tog.m23.p.clk: BOOLEAN,      		{Input-Flanke}
	tog.m23.p.rst: BOOLEAN,
	tog.m23.p.out: BOOLEAN,      		{Output-Flanke}
	tog.m23.p.active,
	tog.m23.p.deactive,          		{Status des counter}
	tog.m23.p.value : Anzahl,    		{counter-Wert}
	tog.m23.p.n_rst,
	tog.m23.p.n_out,
	tog.m23.p.n_clk,

	{m24: D-FF mit D=1 und RST}
	tog.m24.p.rst : BOOLEAN,    		{Input-Flanke}
	{tog.m24.p.clk = tog.m23.p.out}
	tog.m24.p.q: BOOLEAN,       		{Output-Flanke}
	tog.m24.p.pegel_0_0,
	tog.m24.p.pegel_0_1,
	tog.m24.p.pegel_1_0,        		{Pegel: (rst, q)}
	tog.m24.p.n_q,              		{Komplementärplatz}
	tog.m24.p.n_rst,

	{m25: OR}
	tog.m25.p.a: BOOLEAN,              	{Input-Flanken}
	tog.m25.p.b: BOOLEAN,
	{tog.m25.p.out = tog.m24.p.rst}    	{Output-Flanke}
	tog.m25.p.pegel_0_0,
	tog.m25.p.pegel_0_1,
	tog.m25.p.pegel_1_0,
	tog.m25.p.pegel_1_1,              	 {alte Pegel}
	tog.m25.p.n_a,
	tog.m25.p.n_b,

	{m26: Mutex}
	{tog.m26.p.a_in = p.REQ_A}
	{tog.m26.p.b_in = tog.m24.p.q}
	{tog.m26.p.a_out = ip.p.REQ_A1}
	tog.m26.p.b_out: BOOLEAN,    		{Output: Flanke}
	tog.m26.p.n_b_out,
	tog.m26.p.pegel_0_0_0_0,     		{zu speichernde Pegel}
	tog.m26.p.pegel_0_1_0_1,
	tog.m26.p.pegel_1_0_1_0,
	tog.m26.p.pegel_1_1_0_1,
	tog.m26.p.pegel_1_1_1_0,

	{m27: D-FF mit D=1 und RST}
	tog.m27.p.rst : BOOLEAN,
	tog.m27.p.n_rst,
	{tog.m27.p.clk = tog.m26.p.b_out}
	{tog.m27.p.q = tog.m25.p.b, tog.p.ST (= ip.p.ST, and.p.a)}
	tog.m27.p.pegel_0_0,    		{Pegel: (rst, q)}
	tog.m27.p.pegel_0_1,
	tog.m27.p.pegel_1_0,


	{Input Port (ip)}

	{Ein- und Ausgabesignale des Input Port als Flanken}
	ip.p.LCLKM   : BOOLEAN,  		{Eingabesignale}
	ip.p.n_LCLKM,
	{ip.p.INT_CLK = ip.m61.p.b, ip.m63.p.b}
	ip.p.REQ_A1  : BOOLEAN,
	ip.p.n_REQ_A1,
	ip.p.STOP    : BOOLEAN,
	ip.p.n_STOP,
	ip.p.ACKI1   : BOOLEAN,
	ip.p.n_ACKI1,
	{ip.p.ST = ip.m69.p.st, neg(ip.m66.p.b)}
	{ip.p.ACK_INT = ip.m66.p.a, ip.m67.p.b}
	{ip.p.DATAV_IN = lsm.p.DATAV_IN}  	{Ausgabesignale}
	{ip.p.REQ_INT = cc.p.REQ_INT, or.p.a, op.p.REQ_INT}
	{ip.p.RST = tog.p.RST, cc.p.RST}
	{ip.p.REQI1 = pcg.p.REQI1}
	{ip.p.ACK_A = p.ACK_A}
	{ip.p.DLE = p.DLE}

	{m61: AND}
	{ip.m61.p.a = ip.p.LCLKM} 		{Input-Flanken}
	ip.m61.p.b: BOOLEAN,
	ip.m61.p.n_b,
	ip.m61.p.out: BOOLEAN,    		{Output-Flanke}
	ip.m61.p.n_out,
	ip.m61.p.pegel_0_0,
	ip.m61.p.pegel_0_1,
	ip.m61.p.pegel_1_0,
	ip.m61.p.pegel_1_1,       		{alte Pegel}

	{m62: D-FF mit D=0 und SET}
	{ip.m62.p.clk = ip.m61.p.out} 		{Input-Flanke}
	ip.m62.p.set : BOOLEAN,
	ip.m62.p.n_set,
	{ip.m62.p.q = ip.p.DATAV_IN}  		{Output-Flanke}
	ip.m62.p.pegel_0_0,        		{Pegel: (set, q)}
	ip.m62.p.pegel_0_1,
	ip.m62.p.pegel_1_1,

	{m63: AND}
	ip.m63.p.a: BOOLEAN,      		{Input-Flanken}
	ip.m63.p.b: BOOLEAN,
	{ip.m63.p.out = ip.m65.p.a}  		{Output-Flanke}
	ip.m63.p.pegel_0_0,       		{alte Pegel}
	ip.m63.p.pegel_0_1,
	ip.m63.p.pegel_1_0,
	ip.m63.p.pegel_1_1,
	ip.m63.p.n_a,
	ip.m63.p.n_b,

	{m64: AND}
	ip.m64.p.a: BOOLEAN,       		{Input-Flanken}
	ip.m64.p.b: BOOLEAN,
	ip.m64.p.n_a,
	ip.m64.p.n_b,
	{ip.m64.p.out = ip.m65.p.b}    		{Output-Flanke}
	ip.m64.p.pegel_0_0,         		{alte Pegel}
	ip.m64.p.pegel_0_1,
	ip.m64.p.pegel_1_0,
	ip.m64.p.pegel_1_1,

	{m65: OR}
	ip.m65.p.a: BOOLEAN,                               {Input-Flanken}
	ip.m65.p.b: BOOLEAN,
	ip.m65.p.n_a,
	ip.m65.p.n_b,
	{ip.m65.p.out = ip.m62.p.set, ip.p.DLE (= p.DLE)}  {Output-Flanke}
	ip.m65.p.pegel_0_0,                                {alte Pegel}
	ip.m65.p.pegel_0_1,
	ip.m65.p.pegel_1_0,
	ip.m65.p.pegel_1_1,

	{m66: AND}
	ip.m66.p.a: BOOLEAN,      		{Input-Flanken}
	ip.m66.p.b: BOOLEAN,
	ip.m66.p.n_a,
	ip.m66.p.n_b,
	{ip.m66.p.out = ip.m68.p.b}  		{Output-Flanke}
	ip.m66.p.pegel_0_0,       		{alte Pegel}
	ip.m66.p.pegel_0_1,
	ip.m66.p.pegel_1_0,
	ip.m66.p.pegel_1_1,

	{m67: AND}
	ip.m67.p.a: BOOLEAN,      		{Input-Flanken}
	ip.m67.p.b: BOOLEAN,
	ip.m67.p.n_a,
	ip.m67.p.n_b,
	{ip.m67.p.out = ip.m68.p.a}  		{Output-Flanke}
	ip.m67.p.pegel_0_0,       		{alte Pegel}
	ip.m67.p.pegel_0_1,
	ip.m67.p.pegel_1_0,
	ip.m67.p.pegel_1_1,

	{m68: OR}
	ip.m68.p.a: BOOLEAN,                    {Input-Flanken}
	ip.m68.p.b: BOOLEAN,
	ip.m68.p.n_a,
	ip.m68.p.n_b,
	{ip.m68.p.out = ip.m69.p.ackc, ip.m64.p.a}    {Output-Flanke}
	ip.m68.p.pegel_0_0,                     {alte Pegel}
	ip.m68.p.pegel_0_1,
	ip.m68.p.pegel_1_0,
	ip.m68.p.pegel_1_1,

	{m69: Input Controller}
	ip.m69.p.0, ip.m69.p.1, ip.m69.p.2,     {Zustände des Input Controllers}
	ip.m69.p.3, ip.m69.p.4, ip.m69.p.5,
	ip.m69.p.6, ip.m69.p.7, ip.m69.p.8,
	{ip.m69.p.req_a1 = ip.p.REQ_A1}      	{Ein- und Ausgabesignale als Flanken}
	ip.m69.p.st     : BOOLEAN,
	ip.m69.p.n_st,
	{ip.m69.p.ack_a = ip.m63.p.a, ip.p.ACK_A (= p.ACK_A)}
	{ip.m69.p.rst = ip.p.RST}
	{ip.m69.p.acken = ip.m64.p.b, ip.m67.p.a}
	{ip.m69.p.req_int = ip.p.REQ_INT}
	ip.m69.p.ackc   : BOOLEAN,
	ip.m69.p.n_ackc,
	{ip.m69.p.stop = ip.p.STOP}
	{ip.m69.p.reqi1 = ip.p.REQI1}
	{ip.m69.p.acki1 = ip.p.ACKI1}


	{Clock Control (cc)}
	{cc.p.LCLKM = neg(cc.m10.p.clk)}
	cc.p.RST : BOOLEAN,
	cc.p.REQ_INT : BOOLEAN,
	{cc.p.STOPI = pcg.p.STOPI}
	{cc.p.STOP = ip.p.STOP}
	cc.p.n_RST,
	cc.p.n_REQ_INT,

	{m10: Counter}
	cc.m10.p.clk: BOOLEAN,                		{Input-Flanke}
	cc.m10.p.n_clk,
	{cc.m10.p.rst = cc.p.RST}
	{cc.m10.p.out = cc.p.STOP, cc.m11.p.clk}    	{Output-Flanke}
	cc.m10.p.active,                      		{Status des counter}
	cc.m10.p.deactive,
	cc.m10.p.value : Anzahl,              		{counter-Wert}

	{m11: D-FF mit D=1 und RST}
	cc.m11.p.clk : BOOLEAN,    			{Input-Flanke}
	cc.m11.p.n_clk,
	{cc.m11.p.rst = cc.p.REQ_INT}
	{cc.m11.p.q = cc.p.STOPI}    			{Output-Flanke}
	cc.m11.p.pegel_0_0,
	cc.m11.p.pegel_0_1,
	cc.m11.p.pegel_1_0,        			{Pegel: (rst, q)}



	{Pausable Clock Generator (pcg)}

	pcg.p.REQI1: BOOLEAN,
	pcg.p.n_REQI1,
	{pcg.p.ACKI1: = ip.p.ACKI1}
	pcg.p.STRETCH: BOOLEAN,
	pcg.p.n_STRETCH,
	pcg.p.STOPI: BOOLEAN,
	pcg.p.n_STOPI,
	{pcg.p.LCLK = tog.p.LCLK, and.p.b}

	{m1: NOR}
	pcg.m1.p.a: BOOLEAN,                             {Input-Flanken}
	pcg.m1.p.n_a,
	{pcg.m1.p.b = pcg.p.STOPI}
	{pcg.m1.p.out = pcg.m2.p.b_in, pcg.m3.p.b_in, pcg.m5.p.b}   {Output-Flanke}
	pcg.m1.p.pegel_0_0,
	pcg.m1.p.pegel_0_1,
	pcg.m1.p.pegel_1_0,
	pcg.m1.p.pegel_1_1,                              {alte Pegel}

	{m2: Mutex}
	{pcg.m2.p.a_in = pcg.p.REQI1}
	pcg.m2.p.b_in: BOOLEAN,
	pcg.m2.p.n_b_in,
	{pcg.m2.p.a_out = pcg.p.ACKI1}    		{Output: Flanke}
	{pcg.m2.p.b_out = pcg.m4.p.b}
	pcg.m2.p.pegel_0_0_0_0,       			{zu speichernde Pegel}
	pcg.m2.p.pegel_0_1_0_1,
	pcg.m2.p.pegel_1_0_1_0,
	pcg.m2.p.pegel_1_1_0_1,
	pcg.m2.p.pegel_1_1_1_0,

	{m3: Mutex}
	{pcg.m3.p.a_in = pcg.p.STRETCH}
	pcg.m3.p.b_in: BOOLEAN,
	pcg.m3.p.n_b_in,
	{pcg.m3.p.a_out entfaellt}
	{pcg.m3.p.b_out = pcg.m4.p.a}    		{Output: Flanke}
	pcg.m3.p.pegel_0_0_0_0,
	pcg.m3.p.pegel_0_1_0_1,      			{zu speichernde Pegel}
	pcg.m3.p.pegel_1_0_1_0,
	pcg.m3.p.pegel_1_1_0_1,
	pcg.m3.p.pegel_1_1_1_0,

	{m4: OR}
	pcg.m4.p.a: BOOLEAN,
	pcg.m4.p.n_a,
	pcg.m4.p.b: BOOLEAN,
	pcg.m4.p.n_b,
	{pcg.m4.p.out = pcg.m5.p.a}    			{Output-Flanke}
	pcg.m4.p.pegel_0_0,        			{alte Pegel}
	pcg.m4.p.pegel_0_1,
	pcg.m4.p.pegel_1_0,
	pcg.m4.p.pegel_1_1,

	{m5: MCE}
	pcg.m5.p.a: BOOLEAN,
	pcg.m5.p.n_a,
	pcg.m5.p.b: BOOLEAN,               		{Input-Flanke}
	pcg.m5.p.n_b,
	{pcg.m5.p.out = pcg.m1.p.a, pcg.p.LCLK}    	{Output-Flanke}
	pcg.m5.p.pegel_0_0_0,
	pcg.m5.p.pegel_0_1_0,              		{alte Pegel (a_b_c)}
	pcg.m5.p.pegel_1_0_0,
	pcg.m5.p.pegel_0_1_1,
	pcg.m5.p.pegel_1_0_1,
	pcg.m5.p.pegel_1_1_1,



	{AND (and)}
	and.p.a: BOOLEAN,      						{Input-Flanken}
	and.p.b: BOOLEAN,
	and.p.n_a,
	and.p.n_b,
	{and.p.out = ip.p.LCLKM, or.p.b, op.p.LCLKM, cc.p.LCLKM}    	{Output-Flanke}
	and.p.pegel_0_0,       {alte Pegel}
	and.p.pegel_0_1,
	and.p.pegel_1_0,
	and.p.pegel_1_1,



	{OR (or)}
	or.p.a: BOOLEAN,      						{Input-Flanken}
	or.p.b: BOOLEAN,
	or.p.n_a,
	or.p.n_b,
	{or.p.out = ip.p.INT_CLK, lsm.p.INT_CLK, op.p.INT_CLK}    	{Output-Flanke}
	or.p.pegel_0_0,       						{alte Pegel}
	or.p.pegel_0_1,
	or.p.pegel_1_0,
	or.p.pegel_1_1;



	{Output Port (op)}
	{op.p.INT_CLK = op.m2.p.clk, op.m3.p.clk}
	{op.p.DATAV_OUT = neg(op.m2.p.d), op.m3.p.d}
	op.p.LCLKM: BOOLEAN,
	op.p.n_LCLKM,
	op.p.REQ_INT: BOOLEAN,
	op.p.n_REQ_INT,
	{op.p.ACK_INT = ip.p.ACK_INT}
	{op.p.REQ_B = p.REQ_B}
	{op.p.ACK_B = p.ACK_B}
	{op.p.STRETCH = pcg.p.STRETCH}

	{m1: OR}
	{op.m1.p.a = op.p.LCLKM}    					{Input-Flanken}
	{op.m1.p.b = op.p.REQ_INT}
	{op.m1.p.out = neg(op.m2.p.rst), neg(op.m3.p.rst), op.m4.p.clk) {Output-Flanke}
	op.m1.p.pegel_0_0,       {alte Pegel}
	op.m1.p.pegel_0_1,
	op.m1.p.pegel_1_0,
	op.m1.p.pegel_1_1,

	{m2: D-FF mit D beliebig und RST}
	op.m2.p.clk : BOOLEAN,    					{Input-Flanke}
	op.m2.p.d   : BOOLEAN,
	op.m2.p.rst : BOOLEAN,
	op.m2.p.n_clk,
	op.m2.p.n_d,
	op.m2.p.n_rst,
	{op.m2.p.q = op.m4.p.donv}   					{Output-Flanke}
	op.m2.p.pegel_0_0_0,      					{Pegel: (d, rst, q)}
	op.m2.p.pegel_0_0_1,
	op.m2.p.pegel_0_1_0,
	op.m2.p.pegel_1_0_0,
	op.m2.p.pegel_1_0_1,
	op.m2.p.pegel_1_1_0,

	{m3: D-FF mit D beliebig und RST}
	op.m3.p.clk : BOOLEAN,    					{Input-Flanke}
	op.m3.p.d   : BOOLEAN,
	op.m3.p.rst : BOOLEAN,
	op.m3.p.n_clk,
	op.m3.p.n_d,
	op.m3.p.n_rst,
	{op.m3.p.q = op.m4.p.dov}   					{Output-Flanke}
	op.m3.p.pegel_0_0_0,      					{Pegel: (d, rst, q)}
	op.m3.p.pegel_0_0_1,
	op.m3.p.pegel_0_1_0,
	op.m3.p.pegel_1_0_0,
	op.m3.p.pegel_1_0_1,
	op.m3.p.pegel_1_1_0,

	{m4: Output Controller}
	op.m4.p.0,               			{Zustände des Output Controllers}
	op.m4.p.1,
	op.m4.p.2,
	op.m4.p.3,
	op.m4.p.clk: BOOLEAN,    			{Ein- und Ausgabesignale als Flanken}
	op.m4.p.dov: BOOLEAN,
	op.m4.p.donv: BOOLEAN,
	op.m4.p.ack_b: BOOLEAN,
	op.m4.p.n_clk,
	op.m4.p.n_dov,
	op.m4.p.n_donv,
	op.m4.p.n_ack_b,
	{op.m4.p.req_b = op.p.REQ_B, op.m5.p.a, op.m6.p.set}
	{op.m4.p.ack_int = op.p.ACK_INT}

	{m5: OR}
	op.m5.p.a: BOOLEAN,      				{Input-Flanken}
	op.m5.p.b: BOOLEAN,
	op.m5.p.n_a,
	op.m5.p.n_b,
	{op.m5.p.out = neg(op.m6.p.clk)}    			{Output-Flanke}
	op.m5.p.pegel_0_0,       				{alte Pegel}
	op.m5.p.pegel_0_1,
