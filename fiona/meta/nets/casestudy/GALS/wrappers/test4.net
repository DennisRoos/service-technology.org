TRANSITION t.req_a
VAR x: BOOLEAN;
CONSUME p.ACK_A: x, p.n_REQ_A: 1;
PRODUCE p.REQ_A: neg(x), p.n_ACK_A: 1;


TRANSITION t.ack_b
