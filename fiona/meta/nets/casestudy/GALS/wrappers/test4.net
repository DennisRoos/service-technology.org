{Locally Synchronous Module (lsm)}

TRANSITION lsm.datav_in
VAR d, x: BOOLEAN;
CONSUME lsm.p.DATAV_IN: d, lsm.p.datav_pegel: x;
PRODUCE lsm.p.datav_pegel: d, lsm.p.n_DATAV_IN: 1;

TRANSITION lsm.clk_0
CONSUME lsm.p.INT_CLK: ff();
PRODUCE lsm.p.n_INT_CLK: 1;

TRANSITION lsm.clk_1
VAR d: BOOLEAN;
CONSUME lsm.p.INT_CLK: tt(), lsm.p.datav_pegel: d, op.m2.p.n_d: 1,
        op.m3.p.n_d: 1;
PRODUCE op.m2.p.d: neg(d), op.m3.p.d: d, lsm.p.datav_pegel: d,
        lsm.p.n_INT_CLK: 1;


