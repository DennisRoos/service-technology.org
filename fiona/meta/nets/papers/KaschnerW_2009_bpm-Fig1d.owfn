PLACE
  INTERNAL
    p1, p2, p3, p4, p5;

  INPUT
    i, o, s;

  OUTPUT
    a, r;

INITIALMARKING
  p1;

FINALCONDITION
  ((p5 = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME p1, s;
  PRODUCE p2;

TRANSITION t2
  CONSUME p1, o;
  PRODUCE p3;

TRANSITION t3
  CONSUME p3;
  PRODUCE p1, r;

TRANSITION t4
  CONSUME p2;
  PRODUCE p4, a;

TRANSITION t5
  CONSUME p3;
  PRODUCE p4, a;

TRANSITION t6
  CONSUME p4, i;
  PRODUCE p5;

