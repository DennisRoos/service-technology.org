PLACE
  INTERNAL
    p5, p6, p7, p8;

  INPUT
    order, money;

  OUTPUT
    food;

INITIALMARKING
  p5;

FINALCONDITION
  ((p8 = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION accept
  CONSUME p5, order;
  PRODUCE p6;

TRANSITION collect
  CONSUME p6, money;
  PRODUCE p7;

TRANSITION cook
  CONSUME p7;
  PRODUCE p8, food;
