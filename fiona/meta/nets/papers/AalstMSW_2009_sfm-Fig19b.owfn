PLACE
  INTERNAL
    p1, p2, p3, p4;

  INPUT
    s, b;

  OUTPUT
    r, a;

INITIALMARKING
  p1;

FINALCONDITION
  (ALL_PLACES_EMPTY);


TRANSITION t3
  CONSUME p1;
  PRODUCE r, p2;

TRANSITION t4
  CONSUME p2, b;
  PRODUCE p3;

TRANSITION t5
  CONSUME p3;
  PRODUCE p4, a;

TRANSITION t6
  CONSUME p4, s;
  PRODUCE;
