PLACE
  INTERNAL
    p1, p2, p3, p4;

  INPUT
    f, g;

  OUTPUT
    e, h;

INITIALMARKING
  p1;

FINALCONDITION
  (((p3 = 1) AND (p4 = 1)) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME p1, f;
  PRODUCE p3, e;

TRANSITION t2
  CONSUME p1, g;
  PRODUCE p2, e;

TRANSITION t3
  CONSUME p2;
  PRODUCE p4, h;
