PLACE
  INTERNAL
    p1, p2, p3, p4, p5, p6;

  INPUT
    b, c;

  OUTPUT
    a, d;

INITIALMARKING
  p1, p3;

FINALCONDITION
  (((p2 = 1) AND ((p5 = 1) AND (p6 = 1))) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME p1;
  PRODUCE p2, a;

TRANSITION t2
  CONSUME p3, b;
  PRODUCE p4;

TRANSITION t3
  CONSUME p3, c;
  PRODUCE p6;

TRANSITION t4
  CONSUME p4;
  PRODUCE p5, d;
