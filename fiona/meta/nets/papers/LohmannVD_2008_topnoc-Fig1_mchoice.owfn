PLACE
  INTERNAL
    c, p1, p2;

  INPUT
    i1;

  OUTPUT
    o1, o2;

INITIALMARKING;

FINALCONDITION
  ALL_PLACES_EMPTY;


TRANSITION A
  CONSUME i1;
  PRODUCE c;

TRANSITION 1
  CONSUME c;
  PRODUCE p1;

TRANSITION 2
  CONSUME c;
  PRODUCE p2;

TRANSITION 1u2
  CONSUME c;
  PRODUCE p1, p2;

TRANSITION B
  CONSUME p1;
  PRODUCE o1;

TRANSITION C
  CONSUME p2;
  PRODUCE o2;
