PLACE
  INTERNAL
    p1, p2, p3;

  INPUT
    reject, accept;

  OUTPUT
    offer, invoice;

INITIALMARKING
  p1;

FINALCONDITION
  ((p3 = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME p1;
  PRODUCE p2, offer;

TRANSITION t2
  CONSUME p2, reject;
  PRODUCE p1;

TRANSITION t3
  CONSUME p2, accept;
  PRODUCE p3;

