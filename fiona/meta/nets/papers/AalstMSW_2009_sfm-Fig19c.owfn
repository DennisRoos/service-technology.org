PLACE
  INTERNAL
    p1, p2, p3, p4, p5;

  INPUT
    s, b;

  OUTPUT
    r, a;

INITIALMARKING
  p1;

FINALCONDITION
  (((p5 = 1) OR (p5 = 0)) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t3
  CONSUME p1;
  PRODUCE r, p2;

TRANSITION t4
  CONSUME p2;
  PRODUCE p3, a;

TRANSITION t5
  CONSUME p3, b;
  PRODUCE p4;

TRANSITION t6
  CONSUME p4, s;
  PRODUCE;

TRANSITION t7
  CONSUME p2, b;
  PRODUCE p5;
