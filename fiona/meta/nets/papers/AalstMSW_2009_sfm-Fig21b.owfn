PLACE
  INTERNAL
    p1, p2, p3, p4, p5;

  INPUT
    cfood, tmoney;

  OUTPUT
    corder, cmoney, tfood;

INITIALMARKING
  p1, p2;

FINALCONDITION
  (ALL_PLACES_EMPTY);


TRANSITION t1
  CONSUME p2;
  PRODUCE p3, corder;

TRANSITION t2
  CONSUME p3, cfood;
  PRODUCE p4, tfood;

TRANSITION t3
  CONSUME p1, tmoney;
  PRODUCE p5;

TRANSITION t4
  CONSUME p4, p5;
  PRODUCE cmoney;
