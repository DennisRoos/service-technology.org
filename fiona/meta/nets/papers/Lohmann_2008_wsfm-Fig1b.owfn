PLACE
  INTERNAL
    p1, p2, p3, p4, p5;

  INPUT
    d;

  OUTPUT
    a, b, c;

INITIALMARKING
  p1;

FINALCONDITION
  ((p5 = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME p1;
  PRODUCE p2, a;

TRANSITION t2
  CONSUME p2;
  PRODUCE p3, c;

TRANSITION t3
  CONSUME p3, d;
  PRODUCE p4;

TRANSITION t4
  CONSUME p4;
  PRODUCE p5, b;

