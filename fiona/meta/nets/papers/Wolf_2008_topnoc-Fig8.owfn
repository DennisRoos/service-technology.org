PLACE
  INTERNAL
    p1, p2, p3, p4, a, b;

  INPUT
    b1;

  OUTPUT
    a1;

INITIALMARKING
  p1;

FINALCONDITION
  ((p3 = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME p1;
  PRODUCE p2, a;

TRANSITION t2
  CONSUME p2, b;
  PRODUCE p3;

TRANSITION t3
  CONSUME a;
  PRODUCE p4, a1;

TRANSITION t4
  CONSUME p4, b1;
  PRODUCE b;
