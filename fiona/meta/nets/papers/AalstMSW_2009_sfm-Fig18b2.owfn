PLACE
  INTERNAL
    p1, p2, p3, p4;

  INPUT
    r, a;

  OUTPUT
    s, b;

INITIALMARKING;

FINALCONDITION
  (ALL_PLACES_EMPTY);


TRANSITION t1
  CONSUME r;
  PRODUCE p1, p2;

TRANSITION t2
  CONSUME p1, a;
  PRODUCE p3;

TRANSITION t3
  CONSUME p2;
  PRODUCE p4, b;

TRANSITION t4
  CONSUME p3, p4;
  PRODUCE s;
