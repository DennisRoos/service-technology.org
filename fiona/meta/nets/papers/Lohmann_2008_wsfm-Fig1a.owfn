PLACE
  INTERNAL
    alpha, beta, gamma, delta, omega;

  INPUT
    a, b, c;

  OUTPUT
    d;

INITIALMARKING
  alpha;

FINALCONDITION
  ((omega = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME alpha, a;
  PRODUCE beta;

TRANSITION t2
  CONSUME alpha, b;
  PRODUCE delta;

TRANSITION t3
  CONSUME beta, c;
  PRODUCE gamma;

TRANSITION t4
  CONSUME gamma;
  PRODUCE beta, d;

TRANSITION t5
  CONSUME beta, b;
  PRODUCE omega;

