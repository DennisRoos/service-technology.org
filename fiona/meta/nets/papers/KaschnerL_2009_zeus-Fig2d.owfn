PLACE
  INTERNAL
    p1, p2, p3, p4, p5;

  INPUT
    reject, accept;

  OUTPUT
    offer, invoice;

INITIALMARKING
  p1;

FINALCONDITION
  ((p4 = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION t1
  CONSUME p1;
  PRODUCE p2, offer;

TRANSITION t2
  CONSUME p2;
  PRODUCE p3, invoice;

TRANSITION t3
  CONSUME p3, reject;
  PRODUCE p5;

TRANSITION t4
  CONSUME p3, accept;
  PRODUCE p4;

TRANSITION t5
  CONSUME p5;
  PRODUCE p3, offer;

