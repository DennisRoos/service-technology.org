PLACE
  INTERNAL
    alpha, beta, delta, epsilon, omega;

  INPUT
    euro, bc, bt;

  OUTPUT
    c, t;

INITIALMARKING
  alpha;

FINALCONDITION
  ((omega = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION 1
  CONSUME alpha, euro;
  PRODUCE beta;

TRANSITION 2
  CONSUME beta, bc;
  PRODUCE epsilon;

TRANSITION 3
  CONSUME beta, bt;
  PRODUCE delta;

TRANSITION 4
  CONSUME epsilon;
  PRODUCE omega, c;

TRANSITION 5
  CONSUME delta;
  PRODUCE omega, t;
