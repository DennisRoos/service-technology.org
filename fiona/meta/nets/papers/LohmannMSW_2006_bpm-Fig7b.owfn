{ 
DESCRIPTION: Structural dead places and transitions are removed
}

PLACE
  INTERNAL
    p1,p2,p4,p6;
  INPUT
    ;
  OUTPUT
    ;
  
INITIALMARKING
  p1,p2;

FINALCONDITION
  (p6=1 AND ALL_OTHER_PLACES_EMPTY);

TRANSITION t1
  CONSUME p1,p2;
  PRODUCE p4;

TRANSITION t2
  CONSUME p1,p2;
  PRODUCE p4;

TRANSITION t4
  CONSUME p4;
  PRODUCE p6;


