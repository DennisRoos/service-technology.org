PLACE
  INTERNAL
    p5, p6, p7, p8;

  INPUT
    order, money;

  OUTPUT
    food, song;

INITIALMARKING
  p5;

FINALCONDITION
  ((p8 = 1) AND ALL_OTHER_PLACES_EMPTY);


TRANSITION accept
  CONSUME p5, order;
  PRODUCE p6;

TRANSITION cook
  CONSUME p6;
  PRODUCE p7, food;

TRANSITION collect
  CONSUME p7, money;
  PRODUCE p8;

TRANSITION get_drunk
  CONSUME p6;
  PRODUCE p8, song;
