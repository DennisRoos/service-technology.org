PLACE
  INTERNAL
    p0,p1,p2,p3;

  INPUT
    a,b,d;

  OUTPUT
    c;


INITIALMARKING
  p0:	1;

FINALMARKING
  p3;


TRANSITION t1
  CONSUME p0, a;
  PRODUCE p1, c;

TRANSITION t2
  CONSUME p1;
  PRODUCE p2;

TRANSITION t3
  CONSUME p2;
  PRODUCE p1;

TRANSITION t4
  CONSUME p1, d;
  PRODUCE p3;

TRANSITION t5
  CONSUME p0, b;
  PRODUCE p3;

