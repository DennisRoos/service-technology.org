{ artifical net where partner may cause violation of message bound k=5 }

PLACE
INTERNAL
  p0,
  p1;

INPUT
  a,
  b;

INITIALMARKING
  p0: 10;

FINALCONDITION
  p1 = 10 AND p0 = 0 AND a = 0 AND b = 0;

TRANSITION t1    { ?a }
CONSUME
  a,
  p0:   1;
PRODUCE
  p1 : 1;

TRANSITION t2    { ?b }
CONSUME
  b,
  p0:   1;
PRODUCE
  p1 : 1;
