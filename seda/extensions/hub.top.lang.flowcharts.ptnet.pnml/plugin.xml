<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension point="org.eclipse.ui.popupMenus">
      <?gmfgen generated="true"?>
      <objectContribution
            id="hub.top.lang.flowcharts.diagram.ConvertToPnml"
            nameFilter="*.flow_diagram"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               label="Convert to PNML"
               class="hub.top.editor.eclipse.ui.ConvertDiagramFileAction_FCtoPNML"
               menubarPath="additions"
               enablesFor="1"
               id="hub.top.lang.flowcharts.diagram.ConvertToPnmlAction">
         </action>
         <action
               label="Convert to P/T net diagram"
               class="hub.top.editor.eclipse.ui.ConvertDiagramFileAction_FCtoPTnetDiag"
               menubarPath="additions"
               enablesFor="1"
               id="hub.top.lang.flowcharts.diagram.ConvertToPtnetDiagramAction">
         </action>
      </objectContribution>
   </extension>
</plugin>
