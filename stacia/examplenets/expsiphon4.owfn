
PLACE
  INTERNAL
    p1, p2, p3, p4, q1, q2, q3, q4;

INITIALMARKING
    p1, q1;

NOFINALMARKING

TRANSITION a1
	CONSUME p1,q1;
	PRODUCE p2,q2;

TRANSITION a2
	CONSUME p2,q2;
	PRODUCE p3,q3;

TRANSITION a3
	CONSUME p3,q3;
	PRODUCE p4,q4;

TRANSITION a4
	CONSUME p4,q4;
	PRODUCE p1,q1;

