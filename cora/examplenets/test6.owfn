
PLACE
  INTERNAL
    s1, s2, s3, s4;

INITIALMARKING
    s1;

FINALMARKING
	s1, s2, s3, s4:3;

TRANSITION t1
	CONSUME s1;
	PRODUCE s2,s3;

TRANSITION t2
	CONSUME s2;
	PRODUCE s3,s4;

TRANSITION t3
	CONSUME s3;
	PRODUCE s4,s1;

TRANSITION t4
	CONSUME s4,s2;
	PRODUCE s1;


