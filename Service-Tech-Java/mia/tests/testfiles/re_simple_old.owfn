PLACE
INTERNAL p0, p1, p2, p3, p4, p5;
INPUT a, b, c;
OUTPUT d;

INITIALMARKING p0:1;
FINALCONDITION (p5=1 AND ALL_OTHER_PLACES_EMPTY);

TRANSITION t1
CONSUME p0, a;
PRODUCE p1, p2;

TRANSITION t2
CONSUME p1, b;
PRODUCE p3;

TRANSITION t3
CONSUME p2, c;
PRODUCE p4;

TRANSITION t4
CONSUME p3, p4;
PRODUCE p5, d;



