PLACE
 INTERNAL v0, v1, v2, v3, vf;
 INPUT t, k, e;
 OUTPUT z, g;

INITIALMARKING v0:1;
FINALCONDITION (vf=1 AND ALL_OTHER_PLACES_EMPTY);

TRANSITION t1
CONSUME v0, t;
PRODUCE v1, v2;

TRANSITION t2
CONSUME v0, k;
PRODUCE v2, v3;

TRANSITION t3
CONSUME e, v1, v2;
PRODUCE vf, g;

TRANSITION t4
CONSUME e, v2, v3;
PRODUCE vf, g, z;
