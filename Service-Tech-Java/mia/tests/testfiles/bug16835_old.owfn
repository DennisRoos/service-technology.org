PLACE
 INTERNAL p0,p1,p2,p3,p4,p5,pf;
 INPUT t,k,e;
 OUTPUT z,g;

INITIALMARKING p0:1;
FINALCONDITION (pf=1 AND ALL_OTHER_PLACES_EMPTY);

TRANSITION t1
CONSUME p0,t;
PRODUCE p1, p2;

TRANSITION t2
CONSUME p0,k;
PRODUCE p2, p3;

TRANSITION t3
CONSUME p2, e;
PRODUCE p4;

TRANSITION t4
CONSUME p3;
PRODUCE p5,z;

TRANSITION t5
CONSUME p1, p4;
PRODUCE z, g, pf;

TRANSITION t6
CONSUME p4, p5;
PRODUCE pf, g;


