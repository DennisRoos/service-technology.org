m4_define([TEST_SA2IG], [AT_CHECK([cp TESTFILES/NET .])
                         AT_CHECK([PETRI -osa NET], 0, ignore, ignore)
                         AT_CHECK([SA2IG --input=NET.sa --output=NET.ig.og], 0, ignore, ignore)
                         AT_CHECK([IG2SA --input=NET.ig.og --output=NET.2.sa])
