                         AT_CHECK([FIONA -t og NET], 0, ignore, ignore)
                         AT_CHECK([FIONA -t og NET.owfn], 0, ignore, ignore)
                         AT_CHECK([FIONA -t equivalence NET.og NET.owfn.og], 0, ignore, ignore)
                         AT_KEYWORDS(ig sa)])


