PLACE
  ROLES A, B;
  INTERNAL
    alpha, BA, A1, AB, B2L, omega;

  INPUT
    ;

  OUTPUT
    ;

INITIALMARKING
  alpha;

FINALCONDITION
  TRUE;


TRANSITION B1E
  ROLES B;
  CONSUME alpha;
  PRODUCE BA;




TRANSITION AS
  ROLES A;
  CONSUME BA;
  PRODUCE A1;

TRANSITION AE
  ROLES A;
  CONSUME A1;
  PRODUCE AB;




TRANSITION B2S
  ROLES B;
  CONSUME AB;
  PRODUCE B2L;

TRANSITION B2LE
  ROLES B;
  CONSUME B2L;
  PRODUCE BA;

TRANSITION B2LB
  ROLES B;
  CONSUME B2L;
  PRODUCE omega;
