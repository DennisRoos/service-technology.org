PLACE
  ROLES A, B;
  INTERNAL
    alpha, A1B1, B1LS, B1LE, B1A2, A1LS, A1LE, A2B2, omega;

  INPUT
    ;

  OUTPUT
    ;

INITIALMARKING
  alpha;

FINALCONDITION
  TRUE;


TRANSITION A1E
  ROLES A;
  CONSUME alpha;
  PRODUCE A1B1;

TRANSITION B1S
  ROLES B;
  CONSUME A1B1;
  PRODUCE B1LS;

TRANSITION B1L1
  ROLES B;
  CONSUME B1LS;
  PRODUCE B1LE;

TRANSITION B1L2
  ROLES B;
  CONSUME B1LE;
  PRODUCE B1LS;

TRANSITION B1LB
  ROLES B;
  CONSUME B1LE;
  PRODUCE B1A2;




TRANSITION A1S
  ROLES A;
  CONSUME B1A2;
  PRODUCE A1LS;

TRANSITION A1L1
  ROLES A;
  CONSUME A1LS;
  PRODUCE A1LE;

TRANSITION A1L2
  ROLES A;
  CONSUME A1LE;
  PRODUCE A1LS;

TRANSITION A1LA
  ROLES A;
  CONSUME A1LE;
  PRODUCE A2B2;


TRANSITION B2S
  ROLES B;
  CONSUME A2B2;
  PRODUCE omega;

