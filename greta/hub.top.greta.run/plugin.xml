<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            id="hub.top.GRETA.run.actionSet1"
            label="Adaptive System Actions"
            visible="true">
         <action
               class="hub.top.greta.run.actions.AdaptiveProcessSimulation"
               disabledIcon="icons/full/dtool16/run_exc.gif"
               icon="icons/full/etool16/run_exc.gif"
               id="hub.top.GRETA.run.simulation"
               label="start simulation"
               menubarPath="hub.top.GRETA.menus.gretaMenu/execute"
               style="push"
               toolbarPath="hub.top.GRETA.toolbars.gretaToolbar/execute"
               tooltip="start simulation of adaptive process">
         </action>
         <action
               class="hub.top.greta.run.actions.AdaptiveProcessStep"
               disabledIcon="icons/adaptiveProcessStepDisabled4.png"
               icon="icons/adaptiveProcessStepEnabled4.png"
               id="hub.top.GRETA.run.step"
               label="go one step in adaptive process"
               menubarPath="hub.top.GRETA.menus.gretaMenu/execute"
               style="push"
               toolbarPath="hub.top.GRETA.toolbars.gretaToolbar/execute"
               tooltip="go one step in adaptive process">
         </action>
         <!--
         <action
               class="hub.top.greta.run.actions.AdaptiveProcessChooseEvent"
               disabledIcon="icons/FireEventDisabled.png"
               icon="icons/FireEventEnabled2.png"
               id="hub.top.GRETA.run.chooseEvent"
               label="extend adaptive process"
               menubarPath="hub.top.GRETA.menus.gretaMenu/execute"
               style="push"
               toolbarPath="hub.top.GRETA.toolbars.gretaToolbar/execute"
               tooltip="extend adaptive process">
         </action>
         -->
         <action
               class="hub.top.greta.run.actions.AdaptiveProcessRun"
               disabledIcon="icons/full/dlcl16/resume_co.gif"
               icon="icons/full/elcl16/resume_co.gif"
               id="hub.top.GRETA.run.process"
               label="step in adaptive process"
               menubarPath="hub.top.GRETA.menus.gretaMenu/execute"
               state="false"
               style="push"
               toolbarPath="hub.top.GRETA.toolbars.gretaToolbar/execute"
               tooltip="step in adaptive process">
         </action>
         <action
               class="hub.top.greta.run.actions.AdaptiveProcessReset"
               disabledIcon="icons/full/dlcl16/refresh.gif"
               icon="icons/full/elcl16/refresh.gif"
               id="hub.top.GRETA.run.reset"
               label="reset current run"
               menubarPath="hub.top.GRETA.menus.gretaMenu/execute"
               state="false"
               style="push"
               toolbarPath="hub.top.GRETA.toolbars.gretaToolbar/execute"
               tooltip="reset current run">
         </action>
         <action
               class="hub.top.greta.run.actions.CheckOclets"
               disabledIcon="icons/wellformedOcletsOK4.png"
               icon="icons/wellformedOcletsFrage1.png"
               id="hub.top.GRETA.run.checkOclets"
               label="check wellformedness of oclets"
               menubarPath="hub.top.GRETA.menus.gretaMenu/execute"
               style="push"
               toolbarPath="hub.top.GRETA.toolbars.gretaToolbar/execute"
               tooltip="check wellformedness of oclets">
         </action>
         <action
               class="hub.top.greta.run.actions.OcletDependencies"
               id="hub.top.GRETA.run.ocletDependencies"
               label="check dependencies"
               menubarPath="hub.top.GRETA.menus.gretaMenu/analysis"
               style="push"
               tooltip="check dependencies of oclets">
         </action>
         <action
               class="hub.top.greta.run.actions.BuildStateSpace"
               id="hub.top.GRETA.run.buildStateSpace"
               label="build transition system"
               menubarPath="hub.top.GRETA.menus.gretaMenu/analysis"
               style="push"
               tooltip="build state space of the system">
         </action>
         <!--
         <action
               class="hub.top.greta.run.actions.BuildBranchingProcess"
               id="hub.top.GRETA.run.buildBranchingProcess"
               label="build branching process"
               menubarPath="hub.top.GRETA.menus.gretaMenu/analysis"
               style="push"
               tooltip="build branching process of the system">
         </action>
         <action
               class="hub.top.greta.run.actions.BuildBranchingProcess2"
               id="hub.top.GRETA.run.buildBranchingProcess2"
               label="build branching process (new)"
               menubarPath="hub.top.GRETA.menus.gretaMenu/analysis"
               style="push"
               tooltip="build branching process of the system">
         </action>
         -->
         <action
               class="hub.top.greta.run.actions.InstantiateSystem"
               id="hub.top.GRETA.run.instantiateSystem"
               label="instantiate system"
               menubarPath="hub.top.GRETA.menus.gretaMenu/model"
               style="push"
               tooltip="instantiate the system">
         </action>
      </actionSet>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               id="hub.top.GRETA.menus.gretaMenu"
               label="Greta"
               mnemonic="G">
            <separator
                  name="execute"
                  visible="true">
            </separator>
            <separator
                  name="analysis"
                  visible="true">
            </separator>
            <separator
                  name="model"
                  visible="true">
            </separator>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <menu
               id="hub.top.GRETA.toolbars.gretaToolbar"
               label="Greta">
            <separator
                  name="execute"
                  visible="true">
            </separator>
            <separator
                  name="analysis"
                  visible="true">
            </separator>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="hub.top.GRETA.run.apDiagramFileActions"
            nameFilter="*.adaptivesystem_diagram"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               id="hub.top.GRETA.run.apDiagramMenu"
               label="Adaptive Processes"
               path="additions">
            <separator
                  name="maintenance">
            </separator>
            <separator
                  name="analysis">
            </separator>
         </menu>
         <action
               class="hub.top.greta.run.actions.FixDiagramFile"
               enablesFor="1"
               id="hub.top.GRETA.run.fixDiagramFileAction"
               label="Fix diagram file"
               menubarPath="hub.top.GRETA.run.apDiagramMenu/maintenance">
         </action>
         <action
               class="hub.top.greta.run.actions.BuildStateSpace"
               enablesFor="1"
               id="hub.top.GRETA.run.buildStateSpace"
               label="build transition system"
               menubarPath="hub.top.GRETA.run.apDiagramMenu/analysis">
         </action>
         <!--
         <action
               class="hub.top.greta.run.actions.InitProcessSimulationView"
               enablesFor="1"
               id="hub.top.GRETA.run.initProcessSimulationViewAction"
               label="Initialize Simulation View"
               menubarPath="hub.top.GRETA.run.apDiagramMenu/maintenance">
         </action>
         -->
      </objectContribution>
      <!--
      <objectContribution
            id="hub.top.GRETA.run.apModelFileActions"
            nameFilter="*.adaptivesystem"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               id="hub.top.GRETA.run.apDiagramMenu"
               label="Adaptive Processes"
               path="additions">
            <separator
                  name="maintenance">
            </separator>
         </menu>
         <action
               class="hub.top.greta.run.actions.InitProcessSimulationView"
               enablesFor="1"
               id="hub.top.GRETA.run.initProcessSimulationViewAction"
               label="Initialize Simulation View"
               menubarPath="hub.top.GRETA.run.apDiagramMenu/maintenance">
         </action>
      </objectContribution>
      -->
   </extension>
