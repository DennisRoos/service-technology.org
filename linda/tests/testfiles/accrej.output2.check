PLACE
  INTERNAL
    p0,p1,p2,w0,wacc,wrej;
  INPUT
    check,
    getinfo,
    sugg;
  OUTPUT
    acc,
    feedback,
    finalrej,
    info,
    rej;

FINALMARKINGS
    F1: acc=0, check=0, feedback=0, finalrej=0, getinfo=0, info=0, p0=0, p1=0, p2=0, rej=0, sugg=0, w0=1, wacc=0, wrej=0;
