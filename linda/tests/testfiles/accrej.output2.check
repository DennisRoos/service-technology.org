PLACE
  INTERNAL
    p0,p1,p2,w0,wacc,wrej;
  INPUT
    check,
    getinfo,
    sugg;
  OUTPUT
    acc,
    feedback,
    finalrej,
    info,
    rej;

FINALMARKINGS
    F1: p0=0; p1=0; p2=0; w0=0; wacc=1; wrej=0; 

TERMS
