PLACE
INTERNAL
  i, pl3, pl8, f;
INPUT
  FTrigger, FHandle;
OUTPUT
  FDone;

INITIALMARKING
  i: 1;
FINALCONDITION
  (f = 1) AND ALL_OTHER_PLACES_EMPTY;

TRANSITION tr4
  CONSUME FHandle: 1, i: 1;
  PRODUCE pl3: 1;

TRANSITION tr9
  CONSUME FTrigger: 1, pl3: 1;
  PRODUCE pl8: 1;

TRANSITION tr14
  CONSUME pl8: 1;
  PRODUCE f: 1, FDone: 1;

