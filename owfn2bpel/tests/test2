#! /bin/bash

echo ""

echo "test: translation of caise2006.owfn"
echo "      owfn2bpel --input $testdir/testfiles/caise2006.owfn &> /dev/null"
owfn2bpel --input $testdir/testfiles/caise2006.owfn &> /dev/null
result3=$?

echo "test: comparing generated file with expected output"
echo "      diff $testdir/testfiles/caise2006.owfn.bpel $testdir/testfiles/caise2006.owfn.expected"
diff $testdir/testfiles/caise2006.owfn.bpel $testdir/testfiles/caise2006.owfn.expected
result4=$?


if [ $result3 -eq 0 -a $result4 -eq 0 ]
then
  result=0
else
  result=1
fi

exit $result
