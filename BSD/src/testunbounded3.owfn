{ unbounded }

PLACE
INTERNAL
  p1,		
  p2,
  p3,
  p4;
INPUT 
  b;
OUTPUT
  a,
  c;

INITIALMARKING
  p1:	1
 ;

FINALMARKING
  p4:	1
 ;

TRANSITION t1	 { !a }
CONSUME
  p1:	1;	 
PRODUCE
  p2:	1,
  a:	1;

TRANSITION t2	 { ?b }
CONSUME
  b:	1,
  p2:	1;
PRODUCE
  p3:	1;

TRANSITION t3	 { !c }
CONSUME
  p3:	1;
PRODUCE
  c:	1,
  p4:	1;


{ END OF FILE }
