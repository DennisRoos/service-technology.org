{ unbounded }

PLACE
INTERNAL
  p1,		
  p2,
  p3,
  p4;
INPUT 
  c;
OUTPUT
  a,
  b;

INITIALMARKING
  p1:	1
 ;

FINALMARKING
  p4:	1
 ;

TRANSITION t1	 { !a }
CONSUME
  p1:	1;	 
PRODUCE
  p2:	1,
  a:	1;

TRANSITION t2	 { tau }
CONSUME
  p2:	1;
PRODUCE
  p1:	1;

TRANSITION t3	 { !b }
CONSUME
  p2:	1;
PRODUCE
  p3:	1,
  b:	1;

TRANSITION t4	 { ?c }
CONSUME
  p3:	1,
  c:	1;
PRODUCE
  p4 : 1;

