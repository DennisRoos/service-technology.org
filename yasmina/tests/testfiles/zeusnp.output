PLACE
  INTERNAL
    p0,p1,p2,p3,p4,p5,p6;
  INPUT
    a1,
    c1;
  OUTPUT
    b1,
    b2;

FINALMARKINGS
    F1: p0=0, p1=0, p2=0, p3=0, p4=0, p5=0, p6=1;

TERMS
    T1: +1*a1 ;
    T2: +1*a1 +1*b1 ;
    T3: +1*a1 -1*b1 ;
    T4: +1*a1 +1*b2 ;
    T5: +1*a1 -1*b2 ;
    T6: +1*a1 +1*c1 ;
    T7: +1*a1 -1*c1 ;
    T8: +1*b1 ;
    T9: +1*b1 +1*b2 ;
    T10: +1*b1 -1*b2 ;
    T11: +1*b1 +1*c1 ;
    T12: +1*b1 -1*c1 ;
    T13: +1*b2 ;
    T14: +1*b2 +1*c1 ;
    T15: +1*b2 -1*c1 ;
    T16: +1*c1 ;

BOUNDS
    F1,T1: 1,unbounded;
    F1,T2: 2,unbounded;
    F1,T3: 0,unbounded;
    F1,T4: 2,unbounded;
    F1,T5: 0,unbounded;
    F1,T6: 2,unbounded;
    F1,T7: 0,unbounded;
    F1,T8: 1,1;
    F1,T9: 2,2;
    F1,T10: 0,0;
    F1,T11: 2,2;
    F1,T12: 0,0;
    F1,T13: 1,1;
    F1,T14: 2,2;
    F1,T15: 0,0;
    F1,T16: 1,1;
