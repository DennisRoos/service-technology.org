PLACE
  INTERNAL
    p0,p1,p2;
  INPUT
    b;
  OUTPUT
    a,
    c;

FINALMARKINGS
    F1: p0=0, p1=0, p2=1;

TERMS
    T1: +1*a ;
    T2: +1*a +1*b ;
    T3: +1*a -1*b ;
    T4: +1*a +1*c ;
    T5: +1*a -1*c ;
    T6: +1*b ;
    T7: +1*b +1*c ;
    T8: +1*b -1*c ;
    T9: +1*c ;

BOUNDS
    F1,T1: 0,unbounded;
    F1,T2: 1,unbounded;
    F1,T3: -1,-1;
    F1,T4: 1,unbounded;
    F1,T5: -1,unbounded;
    F1,T6: 1,unbounded;
    F1,T7: 2,unbounded;
    F1,T8: 0,unbounded;
    F1,T9: 1,1;
